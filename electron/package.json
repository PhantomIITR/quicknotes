{
  "name": "quicknotes",
  "productName": "QuickNotes",
  "version": "0.0.1",
  "description": "A note taker",
  "main": "main.js",
  "author": "Krzysztof Kowalczyk",
  "license": "ISC",
  "build": {
    "appId": "io.quicknotes",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "installerHeaderIcon": "icon.ico"
    },
    "mac": {
      "category": "productivity",
      "icon": "icon.icns"
    },
    "dmg": {
      "icon": "icon.icns",
      "background": "dmg-background.png"
    }
  },
  "devDependencies": {
    "aws-sdk": "2.6.15",
    "electron": "1.4.5",
    "electron-builder": "7.24.1"
  },
  "dependencies": {
    "electron-positioner": "3.0.0",
    "electron-window-state": "3.1.0"
  }
}
