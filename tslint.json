{
  "rules": {
    "linebreak-style": [
      true,
      "LF"
    ],
    "no-trailing-whitespace": true,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-conditional-assignment": false,
    "quotemark": [
      false
    ],
    "semicolon": [
      true,
      "always"
    ]
  }
}
